{
    "name": "techone/tadmin",
    "description": "the admin framework based on thinkphp",  
    "keywords": [        
        "framework",
        "thinkphp",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "TechLee",
            "email": "techlee@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/think-migration": "^2.0",
        "topthink/think-captcha": "^2.0",
        "league/flysystem": "^1.0",
        "techone/support": "0.*",
        "casbin/think-adapter": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "tadmin\\": "src/"
        },
        "files": [
          "helper.php"
        ]
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    }
}
